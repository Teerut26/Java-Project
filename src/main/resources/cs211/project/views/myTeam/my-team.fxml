<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.myTeam.MyTeamController">
    <children>
    <BorderPane fx:id="parentBorderPane" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0" styleClass="primary-color" stylesheets="@../../style/all.css">
    <top>
        <AnchorPane prefHeight="89.0" prefWidth="200.0" style="-fx-background-color: #EAA807;" BorderPane.alignment="CENTER">
            <children>
                <HBox fx:id="NavBarHBox" alignment="CENTER_LEFT" layoutY="-6.0" prefHeight="100.0" prefWidth="1280.0">
                    <padding>
                        <Insets left="35.0" right="35.0" />
                    </padding>
                </HBox>
            </children>
        </AnchorPane>
    </top>
    <left>
        <AnchorPane prefHeight="662.0" prefWidth="321.0" style="-fx-border-color: white; -fx-border-style: solid; -fx-border-width: 0 2 0 0; -fx-background-color: white;" BorderPane.alignment="CENTER">
            <children>
                <VBox fx:id="SideBarVBox" alignment="TOP_CENTER" layoutY="-1.0" prefHeight="627.0" prefWidth="319.0" spacing="10.0" styleClass="primary-color">
                    <padding>
                        <Insets top="10.0" />
                    </padding>
                </VBox>
            </children>
        </AnchorPane>
    </left>
    <right>
    </right>
        <center>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <children>
                    <VBox prefHeight="626.0" prefWidth="496.0">
                        <children>
                            <VBox prefHeight="632.0" prefWidth="494.0" spacing="10.0" styleClass="primary-color">
                                <children>
                                    <Label styleClass="primary-color" text="All team joined">
                                        <font>
                                            <Font size="20.0" />
                                        </font>
                                    </Label>
                                    <TableView fx:id="teamTableView" prefHeight="614.0" prefWidth="494.0" styleClass="secondary-color">
                                        <columns>
                                            <TableColumn prefWidth="75.0" text="C1" />
                                            <TableColumn prefWidth="75.0" text="C2" />
                                        </columns>
                                    </TableView>
                                </children>
                                <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                </padding>
                            </VBox>
                        </children>
                    </VBox>
                    <VBox prefHeight="626.0" prefWidth="467.0" spacing="10.0" styleClass="primary-color">
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                        <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" styleClass="primary-color">
                           <children>
                                  <Label fx:id="teamNameLabel" styleClass="primary-color" text="Team name">
                                      <font>
                                          <Font size="18.0" />
                                      </font>
                                  </Label>
                              <Pane prefHeight="200.0" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="manageTeamButton" mnemonicParsing="false" onAction="#goToManageTeam" prefHeight="34.0" styleClass="btn-primary" text="manage team">
                                 <HBox.margin>
                                    <Insets right="8.0" />
                                 </HBox.margin></Button>
                              <Button fx:id="cancelTeamButton" mnemonicParsing="false" onAction="#onCancelTeam" prefHeight="34.0" prefWidth="83.0" styleClass="btn-delete" text="cancel join" />
                           </children>
                        </HBox>
                            <Pane prefHeight="171.0" prefWidth="445.0" styleClass="secondary-color">
                                <children>
                                    <Label layoutX="20.0" layoutY="27.0" prefHeight="17.0" prefWidth="54.0" styleClass="secondary-color" text="quantity" />
                                    <Label layoutX="19.0" layoutY="52.0" styleClass="secondary-color" text="date start" />
                                    <Label layoutX="20.0" layoutY="79.0" styleClass="secondary-color" text="date end" />
                                    <Label layoutX="21.0" layoutY="124.0" text="from event" />
                              <Label fx:id="teamQuantityLabel" layoutX="101.0" layoutY="27.0" styleClass="secondary-color" text="Label" />
                              <Label fx:id="teamStartDateLabel" layoutX="101.0" layoutY="52.0" styleClass="secondary-color" text="Label" />
                              <Label fx:id="teamEndDateLabel" layoutX="101.0" layoutY="79.0" styleClass="secondary-color" text="Label" />
                              <Label fx:id="eventTeamNameLabel" layoutX="101.0" layoutY="124.0" text="Label" />
                                </children>
                            </Pane>
                        <TabPane prefHeight="465.0" prefWidth="445.0" tabClosingPolicy="UNAVAILABLE">
                          <tabs>
                            <Tab text="Activity">
                              <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="429.0" prefWidth="445.0">
                                       <children>
                                              <TableView fx:id="activityTableView" layoutY="-1.0" prefHeight="323.0" prefWidth="445.0" styleClass="secondary-color">
                                                  <columns>
                                                      <TableColumn prefWidth="75.0" text="Activity Name" />
                                                      <TableColumn prefWidth="75.0" text="Activity Detail" />
                                                  </columns>
                                              </TableView>
                                                <Button fx:id="commentTeamButton" layoutX="186.0" layoutY="337.0" mnemonicParsing="false" onAction="#onViewActivityTeam" prefHeight="35.0" prefWidth="74.0" styleClass="bg-primary" text="comment" />
                                       </children>
                                    </AnchorPane>
                              </content>
                            </Tab>
                            <Tab text="member">
                              <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                       <children>
                                          <TableView fx:id="userTableView" layoutX="1.0" layoutY="-1.0" prefHeight="322.0" prefWidth="444.0" styleClass="secondary-color">
                                            <columns>
                                              <TableColumn prefWidth="75.0" text="name" />
                                              <TableColumn prefWidth="75.0" text="username" />
                                            </columns>
                                          </TableView>
                                       </children>
                                    </AnchorPane>
                              </content>
                            </Tab>
                          </tabs>
                        </TabPane>
                            <HBox alignment="CENTER" prefHeight="81.0" prefWidth="465.0" />
                        </children>
                    </VBox>
                </children>
            </HBox>
        </center>
    </BorderPane>
    </children>


</AnchorPane>
