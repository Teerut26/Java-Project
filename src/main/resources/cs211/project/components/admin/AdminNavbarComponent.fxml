<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1280.0" styleClass="bg-primary" stylesheets="@../../style/all.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs211.project.controllers.components.admin.AdminNavbarComponentController">
   <children>
      <Label prefHeight="46.0" prefWidth="575.0" text="Event Manager for Admin">
         <font>
            <Font name="Verdana Bold" size="36.0" />
         </font>
      </Label>
      <Pane prefHeight="100.0" prefWidth="473.0" />
      <Circle fx:id="userProfileCircle" fill="DODGERBLUE" radius="21.0" stroke="BLACK" strokeType="INSIDE" />
      <Pane prefHeight="100.0" prefWidth="39.0" />
      <Text fx:id="username" onMouseClicked="#goToProfile" strokeType="OUTSIDE" strokeWidth="0.0" text="Teerut">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Pane prefHeight="100.0" prefWidth="58.0" />
      <Button mnemonicParsing="false" onAction="#goToLogin" prefHeight="26.0" prefWidth="94.0" styleClass="btn-error" text="Logout" textFill="WHITE" />
   </children>
   <padding>
      <Insets left="35.0" right="35.0" />
   </padding>
</HBox>
